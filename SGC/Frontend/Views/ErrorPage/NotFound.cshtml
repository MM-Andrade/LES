@model System.Web.Mvc.HandleErrorInfo
@{
    Layout = null;
    ViewBag.Title = "Erro";
}

<html>
<head>
    <title>@ViewBag.Title - Help ME! - Sistema de gestão de chamados</title>
    <link rel="shortcut icon" href="~/images/icone.ico" />
    @Styles.Render("~/Content/NotFound")
    <link href='https://fonts.googleapis.com/css?family=Anton|Passion+One|PT+Sans+Caption' rel='stylesheet' type='text/css'>
</head>

<body>
    <div class="col-lg-12">
        <!-- Error Page -->
        <div class="error">
            <div class="container-fluid">
                <div class="col-lg-12 ground-color text-center">
                    <div class="container-error-404">
                        <div class="clip"><div class="shadow"><span class="digit thirdDigit"></span></div></div>
                        <div class="clip"><div class="shadow"><span class="digit secondDigit"></span></div></div>
                        <div class="clip"><div class="shadow"><span class="digit firstDigit"></span></div></div>
                        <div class="msg">OH!<span class="triangle"></span></div>
                    </div>
                    <h2 class="h1">Ops! Essa página não existe</h2>
                </div>
            </div>
        </div>
        <!-- Error Page -->
    </div>
</body>
@Scripts.Render("~/bundles/jsnotfound")
</html>